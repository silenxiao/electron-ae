{
    "name": "electron-ae",
    "version": "1.0.0",
    "description": "this is ae tool designed on electron platform",
    "main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prestart": "tsc",
        "start": "electron ./bin/app.js",
        "pack": "electron-builder"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/silenxiao/electron-ae.git"
    },
    "keywords": [
        "ae",
        "electron"
    ],
    "build": {
        "productName": "AE",
        "appId": "com.github.silenxiao",
        "asar": true,
        "files": [
            {
                "from": "libs/images",
                "to": "./node_modules/images",
                "filter": "**/**"
            },
            {
                "from": "node_modules/tinify",
                "to": "./node_modules/tinify",
                "filter": "**/**"
            },
            {
                "from": "bin",
                "to": "./",
                "filter": "**/*"
            }
        ],
        "extraResources": [
            {
                "from": "libs/TP",
                "to": "${arch}/../libs/TP",
                "filter": "**/**"
            }
        ],
        "directories": {
            "output": "dist"
        },
        "win": {
            "target": "zip"
        }
    },
    "author": "silenxiao",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/silenxiao/electron-ae/issues"
    },
    "homepage": "https://github.com/silenxiao/electron-ae#readme",
    "devDependencies": {
        "@types/images": "^2.2.2",
        "@types/node": "^12.0.12",
        "electron": "^18.3.7",
        "electron-builder": "^22.4.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "tinify": "^1.6.0-beta.2"
    }
}